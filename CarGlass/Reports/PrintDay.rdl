  <DataSets>
    <DataSet Name="Data">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT a.calhour, cal.* FROM 
(SELECT 9 as calhour UNION ALL SELECT 10 UNION ALL SELECT 11 UNION ALL SELECT 12 UNION ALL SELECT 13 UNION ALL SELECT 14 UNION ALL SELECT 15 UNION ALL SELECT 16 UNION ALL SELECT 17 UNION ALL SELECT 18 UNION ALL SELECT 19 UNION ALL SELECT 20) as a
LEFT JOIN 
(SELECT orders.id, orders.date, orders.hour, status.name as status, marks.name as mark, models.name as model, orders.car_year, orders.phone, stocks.name as stocks, orders.eurocode, orders.comment, GROUP_CONCAT(glass.name SEPARATOR ', ') as glass FROM orders
LEFT JOIN models ON models.id = orders.car_model_id
LEFT JOIN marks ON marks.id = models.mark_id
LEFT JOIN stocks ON stocks.id = orders.stock_id
LEFT JOIN status ON status.id = orders.status_id
LEFT JOIN order_glasses ON order_glasses.order_id = orders.id
LEFT JOIN glass ON order_glasses.glass_id = glass.id
WHERE orders.date = @date AND orders.type IN (@type)
GROUP BY orders.id
) as cal ON a.calhour = cal.hour</CommandText>
        <QueryParameters>
          <QueryParameter Name="type">
            <Value>=Parameters!type</Value>
          </QueryParameter>
          <QueryParameter Name="date">
            <Value>=Parameters!date</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
