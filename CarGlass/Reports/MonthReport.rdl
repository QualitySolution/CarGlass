  <DataSets>
    <DataSet Name="Data">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT orders.type, SUM(summa.cost) as sum, AVG(summa.cost) as avg, COUNT(orders.id)  as count FROM orders
LEFT JOIN (
SELECT order_id, SUM(order_pays.cost) as cost FROM order_pays GROUP BY order_id
) as summa ON summa.order_id = orders.id
WHERE orders.date BETWEEN @start AND @end 
AND orders.type &lt;&gt; 'other'
GROUP BY orders.type </CommandText>
        <QueryParameters>
          <QueryParameter Name="start">
            <Value>=Parameters!start</Value>
          </QueryParameter>
          <QueryParameter Name="end">
            <Value>=Parameters!end</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields>
        <Field Name="type">
          <DataField>type</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="sum">
          <DataField>sum</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="count">
          <DataField>count</DataField>
          <rd:TypeName>System.Int64</rd:TypeName>
        </Field>
        <Field Name="avg">
          <DataField>avg</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Pays">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT services.name as service, SUM(order_pays.cost) as sum, AVG(order_pays.cost) as avg, COUNT(order_pays.id) as count FROM order_pays
LEFT JOIN services ON services.id = order_pays.service_id 
LEFT JOIN orders ON order_pays.order_id = orders.id
WHERE orders.date BETWEEN @start AND @end 
AND orders.type &lt;&gt; 'other'
GROUP BY order_pays.service_id 
ORDER BY services.ordinal</CommandText>
        <QueryParameters>
          <QueryParameter Name="@start">
            <Value>=Parameters!start</Value>
          </QueryParameter>
          <QueryParameter Name="@end">
            <Value>=Parameters!end</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields>
        <Field Name="service">
          <DataField>service</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="sum">
          <DataField>sum</DataField>
          <TypeName>System.Decimal</TypeName>
        </Field>
        <Field Name="avg">
          <DataField>avg</DataField>
          <TypeName>System.Decimal</TypeName>
        </Field>
        <Field Name="count">
          <DataField>count</DataField>
          <TypeName>System.Int32</TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
