  <DataSets>
    <DataSet Name="Data">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT
    store_items.mark_id,
    marks.name AS brand,
    `model_id`,
    models.name AS model,
    `glass_id`,
    glass.name AS glass_type,
    `eurocode`,
    `manufacturer_id`,
    manufacturers.name AS manufacturer,
    `cost`,
    `amount`,
    `placement`,
    `comment`
FROM
    `store_items`
LEFT JOIN marks ON store_items.mark_id = marks.id
LEFT JOIN models ON model_id = models.id
LEFT JOIN glass ON glass_id = glass.id
LEFT JOIN manufacturers ON manufacturer_id = manufacturers.id
WHERE @place IS NULL OR placement = @place
ORDER BY placement, brand, model, glass_id, amount DESC</CommandText>
        <QueryParameters>
          <QueryParameter Name="@place">
            <Value>={?place}</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
